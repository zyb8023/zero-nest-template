# Docker 构建时忽略的文件
# 这些文件不会被打包到 Docker 镜像中

# 源代码（构建后不需要）
src/
*.ts
!*.d.ts

# 开发工具
.vscode/
.idea/
*.swp
*.swo
*~

# 测试文件
test/
*.spec.ts
*.test.ts
coverage/

# 文档
*.md
!README.md
docs/

# Git
.git/
.gitignore
.gitattributes

# 环境变量（通过环境变量注入）
.env
.env.local
.env.*.local

# 日志（容器内会重新创建）
logs/
*.log

# 临时文件
*.tmp
*.temp
.cache/

# 构建产物（会在 Docker 内重新构建）
dist/

# 其他
.DS_Store
node_modules/.cache
